<input class="s_as__ability-panel__input {{cmpClass}}" type="text"
       data-action="inline-edit"
       value="{{value}}"
       {{#if _id}}data-id="{{_id}}"{{/if}}
    {{#if target}}data-target="{{target}}"{{/if}}
    {{#if index}}data-index="{{index}}"{{/if}}
    {{#if field}}data-field="{{field}}"{{/if}}
    {{#if placeholder}}placeholder="{{ localize placeholder}}"{{/if}}
/>
